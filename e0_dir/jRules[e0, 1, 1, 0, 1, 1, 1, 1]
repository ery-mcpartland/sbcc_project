      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) + 
   (j[e0, -1 + n1, 1, 0, n4, 2, 1, 0]*(sp[p, q] - sp[q, q]))/
    ((-2 + d - 2*n4)*(-(mb^2*mc^2) + 2*mc^2*sp[p, q] + sp[p, q]^2 - 
      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) + 
   (n4*j[e0, -1 + n1, 1, 0, 1 + n4, 1, 1, 0]*(sp[p, q] - sp[q, q]))/
    ((-2 + d - 2*n4)*(-(mb^2*mc^2) + 2*mc^2*sp[p, q] + sp[p, q]^2 - 
      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) - 
   ((-1 + n4)*j[e0, n1, 1, 0, n4, 1, 1, 0]*(sp[p, q] - sp[q, q]))/
    ((-2 + d - 2*n4)*(-(mb^2*mc^2) + 2*mc^2*sp[p, q] + sp[p, q]^2 - 
      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) - 
   (j[e0, n1, 1, 0, -1 + n4, 1, 0, 2]*(-mb^2 + 2*sp[p, q] - sp[q, q]))/
    (2*(-2 + d - 2*n4)*(-(mb^2*mc^2) + 2*mc^2*sp[p, q] + sp[p, q]^2 - 
      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) + 
   (n4*j[e0, -2 + n1, 1, 0, 1 + n4, 1, 1, 1]*(mb^2 - 2*sp[p, q] + sp[q, q]))/
    (2*(-2 + d - 2*n4)*(-(mb^2*mc^2) + 2*mc^2*sp[p, q] + sp[p, q]^2 - 
      mc^2*sp[q, q] - 2*sp[p, q]*sp[q, q] + sp[q, q]^2)) - 
   (j[e0, -1 + n1, 1, 0, n4, 1, 0, 2]*(mb^2 - 2*sp[p, q] + sp[q, q]))/
