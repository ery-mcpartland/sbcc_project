    (n5_)?Positive, (n6_)?Positive, (n7_)?Positive] /; 
   n1 == 1 && n2 == 1 && n4 == 1 && n5 == 1 && n6 == 1 && n7 == 1 -> 
  j[e0, 0, 1, n3, 1, 2, 1, 1]/(4 - d + n3) - 
