    (n5_)?Positive, (n6_)?Positive, (n7_)?Positive] /; 
   n2 == 1 && n4 == 1 && n5 == 1 && n6 == 1 && n7 == 1 && 
     !(n1 == 1 || n3 == 0) -> 
  -1/2*((3 - d + n1)*j[e0, -1 + n1, 1, 1 + n3, 1, 1, 1, 1]*
